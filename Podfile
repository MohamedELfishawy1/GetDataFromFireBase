    pod'Firebase/Database'

  target 'GetDataFromFireBaseTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GetDataFromFireBaseUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
